num1 = float(input())
num2 = float(input())
operator = input()

if operator == '+':
    print("Result =", num1 + num2)
elif operator == '-':
    print("Result =", num1 - num2)
elif operator == '*':
    print("Result =", num1 * num2)
elif operator == '/':
    if num2 != 0:
        print("Result =", num1 / num2)
    else:
        print("Division by zero error")
